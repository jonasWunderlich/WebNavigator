{
  "name": "WebNavigator",
  "version": "0.5",
  "manifest_version": 2,
  "description": "This Tool organises the Webhistory taskoriented - Shows , bookmarks and tabs",
  "background" : {
  	"page": "background.html"
  },
  "chrome_url_overrides" : {
    "newtab": "front.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/bib/jquerymin.js"] //, "src/js/z_contentScript.js"]
    }
  ],
  "permissions": ["history", "bookmarks", "tabs", "storage", "webNavigation", "chrome://favicon/", "http://*/*", "https://*/*" ],
  "browser_action": {
    "default_icon": "style/img/icon.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
